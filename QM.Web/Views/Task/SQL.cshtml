@using QM.Core.Common;

<div class="ibox-title">
    <h5>SQL报表 <small>认真填写每项Item</small></h5>
</div>
<div class="ibox-content">
    <form method="get" class="form-horizontal">
        <div class="form-group">
            <label class="col-sm-2 control-label">SQL Header</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" placeholder="多项用;分隔" name="sql_subject">
            </div>
        </div>
        <div class="hr-line-dashed"></div>
        <div class="form-group">
            <label class="col-sm-2 control-label">文件名称</label>
            <div class="col-sm-5">
                <input type="text" placeholder="任务参数" class="form-control" name="sql_filename" id="qm_parms">
            </div>
            <div class="col-sm-5">
                @Html.DropDownList("sql_filetype", QMExtend.GetTaskType(), new { @class = "form-control m-b" })
            </div>
        </div>
    </form>
</div>